<h1>Listing anscores</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @anscores.each do |anscore| %>
      <tr>
        <td><%= link_to 'Show', anscore %></td>
        <td><%= link_to 'Edit', edit_anscore_path(anscore) %></td>
        <td><%= link_to 'Destroy', anscore, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Anscore', new_anscore_path %>
